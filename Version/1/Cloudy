-- Scripts:

local function XJBFOL_fake_script() -- Cloudy.IfMissingFiles 
	local script = Instance.new('LocalScript', Cloudy)

	wait(5)
	print("[1]Downloading extensions..")
	wait(.01)
	warn("Call for: extensions.")
	wait(1)
	print(" ")
	print(" ")
	wait(.01)
	print("[2]Downloading missing files..")
	wait(.05)
	warn("Call for: missing files.")
	wait(1)
	print(" ")
	print(" ")
	wait(.01)
	print("[3]Downloading lua language..")
	wait(.07)
	warn("Call for: lua language.")
	wait(1)
	print(" ")
	print(" ")
	wait(2)
	print("[4]Downloading exploit server..")
	wait(3)
	warn("Call for: exploit server.")
	wait(.01)
	print(" ")
	print(" ")
	wait(2)
	print("[5]Completeing main..")
	wait(.01)
	warn("Call for: main.")
	wait(.01)
	print(" ")
	print(" ")
	wait(1)
	warn("N37 T77 / "..game.Players.LocalPlayer.LocaleId.." | Access supported.")
	wait(1)
	print(" ")
	print(" ")
	wait(1)
	print("[Cloudy]Granting access for: main console..")
	wait(12)
	warn("Call for main console.")
	print(" ")
	print(" ")
	wait(1)
	print("[Cloudy]")
	local GameInformations = {
		["Name"] = game.Name;
		["GameId"] = game.GameId;
		["GameJobId"] = game.JobId;
	}
	wait(.01)
	warn("Call for:")
	warn(GameInformations)
end
coroutine.wrap(XJBFOL_fake_script)()
local function JJDYEMW_fake_script() -- Close.CloseScript 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Visible = false
		wait(.01)
		script.Parent.Parent.Title.Visible = false
		wait(.01)
		script.Parent.Parent.undertext.Visible = false
		wait(.01)
		script.Parent.Parent.copyrightcloudy.Visible = false
		wait(.01)
		script.Parent.Parent:TweenSize(UDim2.new(0,0,0,0),"Out","Quad")
		wait(2)
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(JJDYEMW_fake_script)()
local function BEQVLV_fake_script() -- CloudyAnnnnnc.Effect 
	local script = Instance.new('LocalScript', CloudyAnnnnnc)

	while true do
		script.Parent.UIStroke.Transparency = 0.95
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.90
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.85
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.80
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.75
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.70
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.65
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.60
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.55
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.50
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.55
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.60
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.65
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.70
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.75
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.80
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.85
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.90
		wait(.07)
		script.Parent.UIStroke.Transparency = 0.95
		wait(.07)
		script.Parent.UIStroke.Transparency = 1
		wait(.07)
	end
end
